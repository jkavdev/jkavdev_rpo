create table paciente(
	id integer not null 
		primary key generated always as identity
		(start with 1, increment by 1),
	nome varchar(255),
	rg varchar(255),
	cpf varchar(255),
	sexo varchar(1),
	endereco_id varchar(255),
	email varchar(255),
	telefone varchar(20),
	celular varchar(20),
);